{
  // "include": [
  // "main.py"
  // ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.venv",
    "**/.git",
    "**/.idea"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.10",
  "typeCheckingMode": "basic",
  "reportUnusedCallResult": false,
  "reportOptionalMemberAccess": false,
  "reportOptionalSubscript": false
}
